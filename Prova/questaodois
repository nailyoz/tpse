#include <stdio.h>

int main() {
    int num, num_bits = 0, div;
    scanf("%d", &num);
    div = num;
    while (div > 0) {
        num_bits++;
        div >>= 1;
    }
    int num_shifted = num << (sizeof(num) * 8 - num_bits);
    printf("O número deslocado em binário é ");
    for (int i = 0; i < sizeof(num) * 8; i++) {
        printf("%d", (num_shifted >> (sizeof(num) * 8 - i - 1)) & 1);
    }
    printf("\n");
    return 0;
}
